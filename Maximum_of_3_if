#include <stdio.h>
void main()
{
    int a, b, c;

    printf(" Input any three numbers ");
    scanf("%d%d%d", &a, &b, &c);

    if (a > b && a > c)
    {
        printf(" a is maximum = %d", a);
    }
    if (b > c && b > a)
    {
        printf(" b is maximum = %d", b);
    }
    if (c > a && c > b)
    {
        printf(" c is maximum = %d", c);
    }
}
