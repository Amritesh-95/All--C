#include <stdio.h>

int main()
{
    int y;

    printf(" Entre a year to check it's leap year or not ");
    scanf("%d", &y);

    if ((y % 4 == 0 && y % 100 != 0) || y % 400 == 0)
    {
        printf("Given year is a leap year", y);
    }
    else
    {
        printf("Given year is not a leap year");
    }
    return 0;
}
