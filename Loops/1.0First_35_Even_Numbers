#include <stdio.h>
int main()
{

    int a = 0, i = 1;
    for (; a != 35; i++)
    {
        if (i % 2 == 0)
        {
            printf("%d\n", i);
            a++;
        }
    }
    return 0;
}




Using While Loop


#include <stdio.h>
int main()
{
    int a = 0, i = 1;
    while (a != 35)
    {
        if (i % 2 == 0)
        {
            printf("%d\n", i);
            a++;
        }
        i++;
    }
    return 0;
}




Using Do While


#include <stdio.h>
int main()
{
    int a = 0, i = 1;
    do
    {
        if (i % 2 == 0)
        {
            printf("%d\n", i);
            a++;
        }
        i++;
    } while (a != 35);
    return 0;
}
