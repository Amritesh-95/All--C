#include <stdio.h>
void main()
{
    float x, y;

    printf(" Input (x,y) coordinate ");
    scanf("%f%f", &x, &y);

    if (x > 0 && y > 0)
    {
        printf(" First quadrant ");
    }
    else if (x < 0 && y > 0)
    {
        printf(" Second quadrant ");
    }
    else if (x < 0 && y < 0)
    {
        printf(" Third quadrant ");
    }
    else if (x > 0 && y < 0)
    {
        printf(" Fourth quadrant ");
    }
    else
    {
        printf(" ORIGIN ");
    }
}
