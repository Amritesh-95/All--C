#include <stdio.h>
void main()
{
    int a, b;

    printf(" Input any two numbers ");
    scanf("%d%d", &a, &b);

    if (a > b)
    {
        printf(" a is maximum = %d", a);
    }
    else if (a == b)
    {
        printf(" Numbers are equal ");
    }
    else
    {
        printf(" b is maximum ");
    }
}
