#include <stdio.h>
void main()
{
    float P, C, B, M, Comp, T, Per;

    printf(" Input marks of P C B M Comp ");
    scanf("%f%f%f%f%f", &P, &C, &B, &M, &Comp);

    T = (P + C + B + M + Comp);
    Per = ((T / 500) * 100);

    if (Per >= 90)
    {
        printf(" Grade A (Marks,Percentage) = %f \n %f", T, Per);
    }
    else if (Per >= 80)
    {
        printf(" Grade B (Marks,Percentage) = %f \n %f", T, Per);
    }
    else if (Per >= 70)
    {
        printf(" Grade C (Marks,Percentage) = %f \n %f", T, Per);
    }
    else if (Per >= 60)
    {
        printf(" Grade D (Marks,Percentage) = %f \n %f", T, Per);
    }
    else if (Per >= 40)
    {
        printf(" Grade E (Marks,Percentage) = %f \n %f", T, Per);
    }
    else
    {
        printf(" Grade F (Marks,Percentage) = %f \n %f", T, Per);
    }
}
