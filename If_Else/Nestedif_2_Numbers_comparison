#include <stdio.h>
void main()
{
    int a, b;

    printf(" Input any two numbers ");
    scanf("%d%d", &a, &b);

    if (a > b)
    {
        printf(" a is maximum = %d ", a);
    }
    if (b > a)
    {
        printf(" b is maximum = %d ", b);
    }
    if (a == b)
    {
        printf(" both are equal ");
    }
}
