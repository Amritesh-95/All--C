#include <stdio.h>
void main()
{
    float P, C, M, E, O, T, Per;

    printf(" Input marks of P C M E O ");
    scanf("%f%f%f%f%f", &P, &C, &M, &E, &O);

    T = (P + C + M + E + O);
    Per = ((T / 500) * 100);

    if (Per >= 90)
    {
        printf(" Grade A (Marks,Percentage) = %f \n %f", T, Per);
    }
    else if (Per >= 80)
    {
        printf(" Grade B (Marks,Percentage) = %f \n %f", T, Per);
    }
    else if (Per >= 60)
    {
        printf(" Grade C (Marks,Percentage) = %f \n %f", T, Per);
    }
    else
    {
        printf(" Grade D (Marks,Percentage) = %f \n %f", T, Per);
    }
}
