#include <stdio.h>
int main()
{
    int a, b, c;

    printf(" Input three sides of triangle ");
    scanf("%d%d%d", &a, &b, &c);

    if (a + b > c && b + c > a && c + a > b)
    {
        printf(" Given sides validates a triangle ");
    }
    else
    {
        printf(" Given sides doesn't validates a triangle ");
    }
    return 0;
}
