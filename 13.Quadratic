#include <stdio.h>
#include <math.h>
int main()
{
    float a, b, c, d, p, q;

    printf(" Input constants a,b,c ");
    scanf("%f%f%f", &a, &b, &c);
    d = b * b - 4 * a * c;

    if (d >= 0)
    {
        p = (-b + sqrt(d)) / 2 * a;
        q = (-b - sqrt(d)) / 2 * a;
        printf(" Given quadratic has Roots are real = %f \n %f ", p, q);
    }
    else
    {
        printf(" Given quadratic has imaginary roots ");
    }
    return 0;
}
